    "scripts": {
        "start": "vite",
        "build": "vite build",
        "serve": "vite preview",
        "lint": "eslint ./src && prettier --check \"**/**.{ts,js,cjs,tsx}\" \"*.{json,md,yml}\"",
        "test": "playwright test -c playwright-ct.config.ts",
        "test:chromium": "playwright test -c playwright-ct.config.ts --project=chromium",
        "test:firefox": "playwright test -c playwright-ct.config.ts --project=firefox",
        "test:webkit": "playwright test -c playwright-ct.config.ts --project=webkit",
        "test:ui": "playwright test -c playwright-ct.config.ts --ui",
        "test:debug": "playwright test -c playwright-ct.config.ts --debug",
        "test:cov": "playwright test -c playwright-ct.config.ts --project=chromium && node scripts/clean-lcov.js",
        "prepare": "husky",
        "generate-types": "openapi-generator-cli generate --generator-key typescript-rxjs-remote && prettier --write \"src/types/openapi/apis/*.ts\" && prettier --write \"src/types/openapi/models/*.ts\"",
        "generate-types-local": "openapi-generator-cli generate --generator-key typescript-rxjs-local && prettier --write \"src/types/openapi/apis/*.ts\" && prettier --write \"src/types/openapi/models/*.ts\"",
        "generate-types-utils": "openapi-generator-cli generate --generator-key typescript-rxjs-utils && prettier --write \"src/types/openapi/utils/apis/*.ts\" && prettier --write \"src/types/openapi/utils/models/*.ts\""
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@openapitools/openapi-generator-cli": "^2.28.3",
