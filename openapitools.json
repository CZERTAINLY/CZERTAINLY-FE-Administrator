{
    "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
    "spaces": 2,
    "generator-cli": {
        "version": "7.12.0",
        "generators": {
            "typescript-rxjs-local": {
                "generatorName": "typescript-rxjs",
                "inputSpec": "./src/doc-openapi-czertainly-core.yaml",
                "output": "./src/types/openapi",
                "typeMappings": {
                    "set": "Array",
                    "Date": "string"
                },
                "additionalProperties": {
                    "withoutRuntimeChecks": true,
                    "supportsES6": true,
                    "withInterfaces": true,
                    "stringEnums": true,
                    "typescriptThreePlus": true
                }
            },
            "typescript-rxjs-remote": {
                "generatorName": "typescript-rxjs",
                "inputSpec": "https://api.czertainly.com/main/doc-openapi-czertainly-core.yaml",
                "output": "./src/types/openapi",
                "typeMappings": {
                    "set": "Array",
                    "Date": "string"
                },
                "additionalProperties": {
                    "withoutRuntimeChecks": true,
                    "supportsES6": true,
                    "withInterfaces": true,
                    "stringEnums": true,
                    "typescriptThreePlus": true
                }
            },
            "typescript-rxjs-utils": {
                "generatorName": "typescript-rxjs",
                "inputSpec": "https://api.czertainly.com/utils/main/utils-service.yaml",
                "output": "./src/types/openapi/utils",
                "typeMappings": {
                    "set": "Array",
                    "Date": "string"
                },
                "additionalProperties": {
                    "withoutRuntimeChecks": true,
                    "supportsES6": true,
                    "withInterfaces": true,
                    "stringEnums": true,
                    "typescriptThreePlus": true
                }
            }
        }
    }
}
